{
    "name": "artemmelnik/laravelshoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Melnik",
            "email": "artemmelnik989@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "6.2.*",
        "illuminate/session": "6.2.*",
        "illuminate/events": "6.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0 || ~8.0",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Artemmelnik\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artemmelnik\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "gloudemans/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Artemmelnik\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Artemmelnik\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
